<h1>ログイン</h1>
	<%= form_with url: login_path, local: true do |f| %>
		<div>
		<%= f.label :email %>
		<%= f.email_field :email%>
		</div>
		<div>
		<%= f.label :password %>
		<%= f.password_field :password %>
		</div>
		<%= f.submit "ログイン" %>
	<% end %>
<%= link_to "登録ページへ", new_user_path %>
<%= link_to "パスワードお忘れの方はこちら", "#" %>
